{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "zookeeperStorageAccountName": {
      "value": "zookeeperstoragevmss"
    },
    "nifiStorageAccountName": {
      "value": "nifistoragevmss"
    },
    "adminUsername": {
      "value": "adminuser"
    },
    "adminPassword": {
      "value": "azure@123456"
    },
    "virtualNetworkName": {
      "value": "nifi-vnet"
    },
    "addressPrefix": {
      "value": "10.0.0.0/16"
    },
    "subnetName": {
      "value": "default"
    },
    "subnetPrefix": {
      "value": "10.0.0.0/24"
    },
    "nifiVersion": {
      "value": "1.0.0"
    },
    "nifiClusterName": {
      "value": "nifi-arm-cluster"
    },
    "nifiNodeIPAddressPrefix": {
      "value": "10.0.0.1"
    },
    "zookeeperNodeIPAddressPrefix": {
      "value": "10.0.0.4"
    },
    "numberOfZookeeperInstances": {
      "value": 3
    },
    "numberOfNifiInstances": {
      "value": 5
    },
    "zookeeperVmSize": {
      "value": "Standard_D1_V2"
    },
    "nifiVmSize": {
      "value": "Standard_DS3_V2"
    },
    "nifiDnsName": { 
      "value": "nifidemo"
    }
  }
}